<Window x:Class="DelegatesDynamicBrowser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:view="clr-namespace:DelegatesDynamicBrowser.View"
        mc:Ignorable="d"
        Title="Browse dll's methods" Height="800" Width="1000">

    <Grid>
        <TabControl>
            <TabItem Header="BrowseDll" Foreground="Green">
                <view:BrowseDll x:Name="BrowseDll"/>
            </TabItem >
        </TabControl>
    </Grid>
</Window>
