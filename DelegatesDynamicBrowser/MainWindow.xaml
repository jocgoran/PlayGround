<Window x:Class="DelegatesDynamicBrowser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:view="clr-namespace:DelegatesDynamicBrowser.View"
        mc:Ignorable="d"
        Title="Browse dll's methods" Height="600" Width="800">

    <StackPanel Orientation="Vertical" Margin="0,10,0,0">
        <view:Navigation x:Name="Navigation"/>
        <view:Delegates x:Name="Delegates" Grid.Row="1"/>
    </StackPanel>        
</Window>
